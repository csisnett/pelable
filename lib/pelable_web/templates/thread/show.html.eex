<h1>Show Thread</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @thread.title %>
  </li>

  <li>
    <strong>Uuid:</strong>
    <%= @thread.uuid %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @thread.type %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.thread_path(@conn, :edit, @thread) %></span>
<span><%= link "Back", to: Routes.thread_path(@conn, :index) %></span>
